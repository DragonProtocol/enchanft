{
  "name": "ecnft",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint:all": "eslint . --ext .{js,ts,tsx} --cache",
    "lint:fix:all": "pnpm lint --fix",
    "format:all": "prettier --write \"**/*.{js,ts,tsx}\"  --parser typescript ",
    "format:check:all": "prettier --check \"**/*.{js,ts,tsx}\"  --parser typescript ",
    "start:all": "pnpm --stream -r start",
    "build:all": "pnpm --stream -r build",
    "test:all": "pnpm --stream -r test",
    "project:script": "node ./tools/scripts/project.script.mjs",
    "project:exec": "node ./tools/scripts/project.exec.mjs",
    "start": "pnpm project:script start",
    "build": "pnpm project:script build",
    "test": "pnpm project:script test",
    "lint": "pnpm project:script lint",
    "lint:fix": "pnpm project:script lint:fix",
    "format": "pnpm project:script format",
    "format:check": "pnpm project:script format:check",
    "prepare": "husky install",
    "format:staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,md,yml}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "13.3.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.13",
    "@types/react-dom": "18.0.5",
    "@types/react-is": "17.0.3",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "~5.44.0",
    "@typescript-eslint/parser": "~5.44.0",
    "chalk": "^5.1.2",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "husky": "^8.0.0",
    "jest": "27.5.1",
    "jest-watch-typeahead": "^1.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2",
    "web-vitals": "^2.1.4"
  }
}
